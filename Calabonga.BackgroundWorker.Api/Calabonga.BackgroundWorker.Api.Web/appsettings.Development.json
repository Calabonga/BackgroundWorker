{
  "ConnectionStrings": {
    "ApplicationDbContext": "Server=<SQL>;Database=MicroserviceWithIdentityDb;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Microsoft": "Information"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Error",
        "Calabonga": "Information"
      }
    }
  },
  "Cors": {
    "Origins": "*"
  },
  "IdentityServer": {
    "Url": "http://localhost:10001"
  },
  "CurrentAppSettings": {
    "PageSize": 50
  }
}
